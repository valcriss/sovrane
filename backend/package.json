{
  "name": "sovrane-backend",
  "version": "1.0.0",
  "description": "Sovrane application backend",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.ts",
  "scripts": {
    "database:generate": "prisma generate --schema adapters/orm/prisma/schema.prisma",
    "database:update": "prisma migrate dev --schema adapters/orm/prisma/schema.prisma",
    "database:reset": "prisma migrate reset --schema adapters/orm/prisma/schema.prisma",
    "database:format": "prisma format --schema adapters/orm/prisma/schema.prisma",
    "database:fullreset": "rm -rf adapters/orm/prisma/migrations && npm run database:reset && npm run database:update && npm run database:generate",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --watchAll=false",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "dev": "ts-node-dev --respawn --transpile-only infrastructure/server.ts",
    "initialize": "node scripts/init-application.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@prisma/client": "^6.12.0",
    "argon2": "^0.43.1",
    "email-templates": "^12.0.3",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.8",
    "prisma": "^6.12.0",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/email-templates": "^10.0.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/socket.io-client": "^1.4.36",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "jest": "^30.0.5",
    "jest-mock-extended": "^4.0.0",
    "socket.io-client": "^4.7.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
